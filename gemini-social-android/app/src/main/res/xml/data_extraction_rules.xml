<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfUnsupported="true">
        <include domain="file" path="."/>
        <exclude domain="file" path="[PATH_TO_EXCLUDE]"/>
    </cloud-backup>
    <device-transfer disableIfUnsupported="true">
        <include domain="file" path="."/>
        <exclude domain="file" path="[PATH_TO_EXCLUDE]"/>
    </device-transfer>
</data-extraction-rules>
